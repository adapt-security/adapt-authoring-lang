{
  "name": "adapt-authoring-lang",
  "version": "0.0.1",
  "description": "Localisation for the Adapt authoring tool",
  "homepage": "https://github.com/taylortom/adapt-authoring-l10n",
  "main": "index.js",
  "adapt_authoring": {
    "module": true,
    "utility": "lang",
    "moduleDependencies": {
      "adapt-authoring-test": "*"
    },
    "documentation": {
      "enable": true,
      "includes": {
        "source": "lib/*",
        "docs": "docs/*"
      }
    }
  },
  "dependencies": {
    "node-polyglot": "2.3.0"
  },
  "devDependencies": {
    "i18n-extract": "0.6.6"
  }
}
